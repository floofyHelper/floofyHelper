{
  "name": "floofyhelper",
  "version": "1.0.0",
  "main": "build/initial.js",
  "type": "module",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "tsc && node . ",
    "test": "tsc && node --trace-warnings . dev ",
    "format": "prettier . --write",
    "generate": "prisma generate --schema=./src/prisma/schema.prisma"
  },
  "prettier": {
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 2,
    "printWidth": 100,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "@sentry/node": "^7.20.0",
    "chalk": "^5.1.0",
    "discord-verify": "^1.0.0",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.1",
    "dotenv-vault": "^1.13.10",
    "inquirer": "^9.1.3",
    "newrelic": "^9.3.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.2",
    "@types/node": "^18.11.9",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prisma": "^4.6.1",
    "typescript": "^4.8.4"
  }
}
