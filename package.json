{
	"name": "floofyhelper",
	"version": "0.0.1",
	"description": "General-purpose & moderation bot with ✨polish✨. Designed to make management and moderation easy.",
	"main": "src/app/initial.js",
	"type": "module",
	"scripts": {
		"start": "node .",
		"lint": "eslint .",
		"depcheck": "npx depcheck",
		"test": "mocha",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/floofyHelper/floofyHelper"
	},
	"keywords": [
		"Discord",
		"Nodejs",
		"Sapphire-Framework"
	],
	"author": "Acestriker",
	"license": "AGPL-3.0",
	"bugs": {
		"url": "https://github.com/floofyHelper/floofyHelper/issues"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"es2021": true
		},
		"extends": [
			"standard-with-typescript",
			"prettier"
		],
		"overrides": [],
		"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
		},
		"rules": {}
	},
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 4,
		"semi": false,
		"singleQuote": true,
		"endOfLine": "auto",
		"useTabs": true
	},
	"lint-staged": {
		"*.js": "eslint --cache --fix",
		"*.{js,css,md}": "prettier --write"
	},
	"dependencies": {
		"@sapphire/framework": "^3.1.1",
		"chalk": "^5.0.1",
		"cronitor": "^2.3.2",
		"discord.js": "^13.11.0",
		"dotenv": "^16.0.1",
		"js-yaml": "^4.1.0",
		"nsfwjs": "^2.4.1",
		"stop-discord-phishing": "^0.3.1"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"eslint": "^8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard-with-typescript": "^23.0.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-n": "^15.2.5",
		"eslint-plugin-promise": "^6.0.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"mocha": "^10.0.0",
		"prettier": "2.7.1",
		"typescript": "^4.8.3"
	}
}
