{
	"name": "floofyhelper",
	"version": "0.0.1",
	"author": "Acestriker",
	"license": "AGPL-3.0",
	"description": "General-purpose & moderation bot with ✨polish✨. Designed to make management and moderation easy.",
	"keywords": [
		"Discord",
		"Nodejs",
		"Sapphire-Framework"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/floofyHelper/floofyHelper"
	},
	"bugs": {
		"url": "https://github.com/floofyHelper/floofyHelper/issues"
	},
	"main": "src/build/initial.js",
	"type": "module",
	"scripts": {
		"prestart": "tsc",
		"start": "node src/build/initial.js",
		"stop": "killall node",
		"lint": "eslint .",
		"depcheck": "npx depcheck",
		"test": "mocha",
		"i": "husky install",
		"install": "husky install"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"es2021": true
		},
		"extends": [
			"standard-with-typescript",
			"prettier"
		],
		"overrides": [],
		"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
		},
		"ignorePatterns": [
			"src/build/**",
			"src/test/**"
		],
		"rules": {}
	},
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 4,
		"semi": false,
		"singleQuote": true,
		"endOfLine": "lf",
		"useTabs": true
	},
	"lint-staged": {
		"*.js": "eslint --cache --fix",
		"*.{js,css,md}": "prettier --write"
	},
	"dependencies": {
		"chalk": "^5.1.2",
		"cronitor": "^2.3.2",
		"discord.js": "^14.5.0",
		"dotenv": "^16.0.1",
		"dotenv-vault": "^1.13.10",
		"inquirer": "^9.1.3",
		"js-yaml": "^4.1.0",
		"nsfwjs": "^2.4.1",
		"stop-discord-phishing": "^0.3.1",
		"ts-node": "^10.9.1"
	},
	"devDependencies": {
		"@types/inquirer": "^9.0.2",
		"@types/js-yaml": "^4.0.5",
		"@types/node": "^18.8.3",
		"@typescript-eslint/eslint-plugin": "^5.39.0",
		"eslint": "^8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard-with-typescript": "^23.0.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-n": "^15.3.0",
		"eslint-plugin-promise": "^6.0.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "2.7.1",
		"typescript": "^4.8.4"
	}
}
