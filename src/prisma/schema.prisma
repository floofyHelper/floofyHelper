generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["clientExtensions"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model guild {
  id           String         @id @map("_id")
  verification verificationId
}

type verificationId {
  verificationId verificationIdFields
}

type verificationIdFields {
  verificationIdSubmissions String[]
}
